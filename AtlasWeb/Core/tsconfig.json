{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "declaration": false,
    "skipLibCheck": true,
    "outDir": "../dist",
    "baseUrl": ".",
    "paths": {
      "app": [
        "./wwwroot/app"
      ],
      "Modules": [
        "./wwwroot/Modules"
      ],
      "app/*": [
        "./wwwroot/app/*"
      ],
      "AppModules/*": [
        "./wwwroot/app/*"
      ],
      "Fw/*": [
        "./wwwroot/app/Fw/*"
      ],
      "NebulaClient/*": [
        "./wwwroot/app/NebulaClient/*"
      ],
      "System/*": [
        "./wwwroot/app/System/*"
      ],
      "Modules/*": [
        "./Modules/*"
      ],
      "ExternalService/*": [
        "./wwwroot/app/NebulaClient/Services/External/*"
      ],
      "ObjectClassService/*": [
        "./wwwroot/app/NebulaClient/Services/ObjectService/*"
      ],
      "globalscripts": [ "wwwroot/assets/global/scripts" ],
      "tslib": [ "node_modules/tslib/tslib.js" ],
      "file-saver": [ "node_modules/file-saver/FileSaver.min.js" ],
      "globalize$": [ "node_modules/globalize/dist/globalize.js" ],
      "globalize": [ "node_modules/globalize/dist/globalize" ],
      "globalize/currency": [ "node_modules/globalize/dist/globalize/currency.js" ],
      "globalize/date": [ "node_modules/globalize/dist/globalize/date.js" ],
      "globalize/number": [ "node_modules/globalize/dist/globalize/number.js" ],
      "globalize/message": [ "node_modules/globalize/dist/globalize/message.js" ],
      "object-hash": [ "node_modules/object-hash/dist/object_hash.js" ],
      "cldr$": [ "node_modules/cldrjs/dist/cldr.js" ],
      "cldr": [ "node_modules/cldrjs/dist/cldr" ],
      "cldr/*": [
        "node_modules/cldrjs/dist/cldr/*"
      ],
      "cldr/event": [ "node_modules/cldrjs/dist/cldr/event.js" ],
      "cldr/supplemental": [ "node_modules/cldrjs/dist/cldr/supplemental.js" ],
      "object-path": [ "node_modules/object-path" ],
      "devextreme": [ "node_modules/devextreme" ],
      "devextreme/localization/globalize/core": [ "node_modules/devextreme/localization/globalize/core.js" ],
      "devextreme/localization/globalize/number": [ "node_modules/devextreme/localization/globalize/number.js" ],
      "devextreme/localization/globalize/date": [ "node_modules/devextreme/localization/globalize/date.js" ],
      "devextreme/localization/globalize/currency": [ "node_modules/devextreme/localization/globalize/currency.js" ],
      "devextreme/localization/globalize/message": [ "node_modules/devextreme/localization/globalize/message.js" ],
      "ng2-responsive": [ "node_modules/ng2-responsive" ],
      "webcam": [ "node_modules/webcamjs/webcam.min.js" ],
      "jszip": [ "node_modules/jszip/dist/jszip.min.js" ],
      "core-js/es7/reflect": [
        "node_modules/core-js/proposals/reflect-metadata"
      ],
      "core-js/es6/*": [
        "node_modules/core-js/es/*"
      ]
    }
  },
  "include": [
    "wwwroot/app/**/*.ts",
    "Modules/**/*.ts",
    "global.d.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot/dist",
    "**/*.ngfactory.ts",
    "**/*.shim.ts",
    "wwwroot/app/main.aot.ts",
    "wwwroot/app/polyfill.ts",
    "obj",
    "bin"
  ],
  "awesomeTypescriptLoaderOptions": {
    "usePrecompiledFiles": true,
    "useWebpackText": true
  },
  "angularCompilerOptions": {
    "genDir": "../dist/aot",
    "entryModule": "wwwroot/app/app.module#HvlAppModule",
    "enableIvy": true
  }
}
