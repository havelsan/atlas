{
  "name": "atlasweb",
  "description": "Atlas Care",
  "version": "1.0.0",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build:prod": "node --max-old-space-size=16000 ./node_modules/webpack/bin/webpack --config webpack.prod.js --progress --env.production --display-error-details --display-cached",
    "build:aot": "node --max-old-space-size=16000 ./node_modules/webpack/bin/webpack --config webpack.aot.js --display-error-details --display-cached",
    "build:dev": "node --max-old-space-size=16000 ./node_modules/webpack/bin/webpack --config webpack.dev.js --display-error-details --display-cached",
    "build:ngbuild": "\"node --max-old-space-size=16000 ./node_modules/@angular/cli/bin/ng build --aot=true --sourceMap=false ",
    "build:ngserve": "node --max-old-space-size=16000 ./node_modules/@angular/cli/bin/ng serve --aot=true --sourceMap=false",
    "dev-auto": "node --max-old-space-size=16000  ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js --hot --inline --progress --env.NODE_ENV=local --port 8000",
    "dev": "node --max-old-space-size=16000  ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.dev.js --hot-only --inline --progress --env.NODE_ENV=local --port 8000",
    "atlascli": "node --max-old-space-size=16000 ./node_modules/@angular/cli/bin/ng serve",
    "atlaslintwwwroot": "node --max-old-space-size=16000 C:\\Users\\USERNAME\\AppData\\Roaming\\npm\\node_modules\\tslint\\bin\\tslint --fix wwwroot/**/*.ts -c tslint.json -p tsconfig.json",
    "atlaslintmodules": "node --max-old-space-size=16000 C:\\Users\\USERNAME\\AppData\\Roaming\\npm\\node_modules\\tslint\\bin\\tslint --fix Modules/**/*.ts -c tslint.json -p tsconfig.json",
    "atlas-prod": "node --max-old-space-size=13000 ./node_modules/@angular/cli/bin/ng build --configuration=production --aot=true --sourceMap=false",
    "postinstall": "ngcc"
  },
