{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "declaration": false,
    "outDir": "../dist",
    "baseUrl": ".",
    "paths": {
      "*": [ "./*" ],
      "app/*": [ "./wwwroot/app/*" ],
      "Fw/*": [ "./wwwroot/app/Fw/*" ],
      "NebulaClient/*": [ "./wwwroot/app/NebulaClient/*" ],
      "System/*": [ "./wwwroot/app/System/*" ],
      "Modules/*": [ "./Modules/*" ],
      "ExternalService/*": [ "./wwwroot/app/NebulaClient/Services/External/*" ],
      "ObjectClassService/*": [ "./wwwroot/app/NebulaClient/Services/ObjectService/*" ]
    }
  },
  "include": [
    "wwwroot/app/**/*.ts",
    "Modules/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot/app/localization.ts"
  ]
}