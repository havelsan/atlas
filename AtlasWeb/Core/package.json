    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build:prod": "node --max-old-space-size=16384 ./node_modules/webpack/bin/webpack --config webpack.prod.js --progress --env.production --display-error-details --display-cached",
    "build:aot": "node --max-old-space-size=16384 ./node_modules/webpack/bin/webpack --config webpack.aot.js --display-error-details --display-cached",
