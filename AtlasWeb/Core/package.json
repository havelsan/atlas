    "atlascli": "node --max-old-space-size=16384 ./node_modules/@angular/cli/bin/ng serve",
    "build:ngstat": "node --max-old-space-size=12288 ./node_modules/@angular/cli/bin/ng build --sourceMap=false --statsJson=true",
    "build:ngaotstat": "node --max-old-space-size=12288 ./node_modules/@angular/cli/bin/ng build --aot=true --optimization=false --buildOptimizer=false --sourceMap=false --statsJson=true",
    "bundle-report": "node --max-old-space-size=12288 ./node_modules/webpack-bundle-analyzer/lib/bin/analyzer dist/out/stats.json",
    "build:ngbuild": "node --max-old-space-size=12288 ./node_modules/@angular/cli/bin/ng build --aot=true --sourceMap=false --statsJson=true",
    "build:ngserve": "node --max-old-space-size=12288 ./node_modules/@angular/cli/bin/ng serve --aot=true --sourceMap=false --sourceMap=false",
    "atlaslintwwwroot": "node --max-old-space-size=16384 C:\\Users\\USERNAME\\AppData\\Roaming\\npm\\node_modules\\tslint\\bin\\tslint --fix wwwroot/**/*.ts -c tslint.json -p tsconfig.json",
    "atlaslintmodules": "node --max-old-space-size=16384 C:\\Users\\USERNAME\\AppData\\Roaming\\npm\\node_modules\\tslint\\bin\\tslint --fix Modules/**/*.ts -c tslint.json -p tsconfig.json",
    "atlas-prod": "node --max-old-space-size=13000 ./node_modules/@angular/cli/bin/ng build --configuration=production --aot=true --sourceMap=false"
  },
